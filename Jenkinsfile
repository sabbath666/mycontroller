pipeline {    agent any    triggers {        pollSCM('* * * * *')    }    stages {        stage("Checkout") {            steps {                git url: 'https://github.com/sabbath666/mycontroller.git'            }        }        stage("Compile") {            steps {                sh "chmod -R 777 *"                sh "./mvnw install"            }        }//        stage("Unit Test") {//            steps {//                sh "chmod -R 777 *"//                sh "./mvnw test"//            }//        }        stage("Docker build") {            steps {                sh "docker --version"                sh "docker build -t 142.93.68.244/mycontroller ."            }        }        stage("Docker push") {            steps {                sh "docker login -u admin -p hoog0ree 142.93.68.244"                sh "docker push 142.93.68.244/mycontroller"            }        }        stage("Deploy to staging") {            steps {                sh "docker rm -f mycontroller || echo 'my controller doesn't extsts. ok"                sh "docker run -d --rm -p 9999:8080 --name mycontroller 142.93.68.244/mycontroller"            }        }        stage("Acceptance test") {            steps {                sleep 10                sh "./acceptance_test.sh"            }        }    }    post {        always {            sh "docker stop mycontroller"        }    }}